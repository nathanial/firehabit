{
  "name": "personal-life-manager",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@blueprintjs/core": "^1.16.0",
    "auth0-lock": "^10.15.1",
    "draft-js": "^0.10.1",
    "draft-js-prism": "^1.0.3",
    "firebase": "^3.9.0",
    "lodash": "^4.17.4",
    "mobx": "^3.1.9",
    "mobx-react": "^4.1.8",
    "moment": "^2.18.1",
    "node-sass-chokidar": "^0.0.1",
    "npm-run-all": "^4.0.2",
    "react": "^15.5.4",
    "react-addons-css-transition-group": "^15.5.2",
    "react-color": "^2.11.7",
    "react-dnd": "^2.3.0",
    "react-dnd-html5-backend": "^2.3.0",
    "react-dom": "^15.5.4",
    "react-mixin": "^3.0.5",
    "react-router-dom": "^4.1.1",
    "react-scrollbar": "^0.5.1",
    "reactfire": "^1.0.0",
    "styled-components": "^1.4.5",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "react-scripts": "0.9.5"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/ --include-path=./node_modules/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --include-path=./node_modules/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
