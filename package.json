  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/ --include-path=./node_modules/",
    "watch-sass": "node-sass-chokidar src/ -o src/ --include-path=./node_modules/ --watch --recursive",
    "start-js": "node scripts/start.js",
    "build-js": "node scripts/build.js",
    "start": "webpack-dev-server -d --config ./config/webpack.config.dev.js",
    "build": "npm-run-all build-css build-js",
    "test": "cypress run --browser chrome",
    "cypress:open": "cypress open"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scripts)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
